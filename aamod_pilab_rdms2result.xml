<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='subject' desc='convert pilab_data_rdms_mean to pilab_result' modality='MRI'>
            
            <qsub>
                <timeBase>0.5</timeBase>
                <memoryBase>0.3</memoryBase>
            </qsub>
            
            <permanenceofoutput>3</permanenceofoutput>

            <inputstreams>
                <stream>pilab_data_rdms_mean</stream>
            </inputstreams>
            <outputstreams>
                <stream>pilab_result</stream>
                <stream>pilab_result_nulldist</stream>
                <stream>pilab_result_bootdist</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>
